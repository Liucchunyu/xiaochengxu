.city-list-wrap{
    padding: 0 32rpx;
}
.city-list-item{
    padding: 28rpx;
    background: #ffffff;
    position: relative;
    border-bottom: 2rpx #D9D9D9 dashed;
}
.city-list-wrap .city-list-item:last-child{
    border-bottom: 0;
}
.city-list-item .flex-item{
    display: flex;
    padding-top: 10rpx;
    position: relative;
    padding-left: 80rpx;
    position: relative;
}
.city-list-item .check{
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    top: 50%;
    margin-top: -20rpx;
}
.city-list-item .check img{
    width: 100%;
    height: 100%;
}
.city-list-item .flex-item text{
    flex: 1;
    color: #555963;
    font-size: 26rpx;
}
.gray .flex-item text{
    color: #ccc !important;
}
.city-list-item .flex-item .label{
    width: 140rpx;
    flex: 0 0 auto;
}
.city-list-item .flex-item .label-person{
    margin-left: 10px;
    border: 2rpx solid #0091FF;
    border-radius: 2rpx;
    font-size: 22rpx;
    padding: 0 6rpx;
    color: #0091FF !important;
    height: 32rpx;
}
.letter-side-bar {
    position: fixed;
    top: 100rpx;
    right: 8rpx;
    font-size: 28rpx;
    height: 100vh;
    width: 30rpx;
    z-index: 10;
    text-align: center;
    z-index: 1001;
}

.letter-side-bar .letter-item {
    color: #0091FF;
    margin-bottom: 8rpx
}

.letter-side-bar .letter-item.active {
    color: #fff;
    background-color: #0091FF
}

.result-wrap {
    z-index: 1002;
    height: calc(100vh - 100rpx);
    width: 100%;
    position: fixed;
    background-color: #fff;
    bottom: 0;
    left: 0;
    overflow-y: auto;
    display: none;
    box-sizing: border-box;
}

.result-wrap.active {
    display: block
}

.result-item {
    height: 88rpx;
    margin-left: 31rpx;
    background-color: #fff;
    color: #000;
    font-size: 28rpx;
    line-height: 88rpx;
    border-bottom: 1rpx solid #eee
}

.multiple-box {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    padding: 32rpx 32rpx 12rpx;
    
    background-color: #ffffff;
    border-top: 2rpx #ccc solid;
    
    /* max-height: 300rpx;
    overflow-y: auto; */
    
    z-index: 1003;
}
.fixed{
    top: 0;
}
.fixed .multiple-box-list{
    height: auto;
}

.multiple-box .title{
    padding-bottom: 20rpx;
    position: relative;
}
.multiple-box .shrink{
    position: absolute;
    right: 20rpx;
    background-color: #F3F3F3;
    width: 50rpx;
    height: 50rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    top: -10rpx;
    border-radius: 100%;
    transform: rotate(0deg);
    
}
.fixed .shrink{
    transform: rotate(180deg);
}
.multiple-box .shrink image{
    width: 30rpx;
    height: 30rpx;
}
.multiple-box-list{
    display: flex;
    flex-wrap: wrap;
    margin-bottom: 100rpx;
    height: 70rpx;
    overflow: hidden;
}

.multiple-box .item {
    background: #F7F7F7;
    border-radius: 80rpx;
    padding: 0 16rpx 0 26rpx;
    height: 60rpx;
    line-height: 60rpx;
    display: flex;
    align-items: center;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}

.multiple-box .item image {
    width: 32rpx;
    height: 32rpx;
}

.multiple-box .sureBtn {
    width: 60%;
    border-radius: 50rpx;
    line-height: 70rpx;
    position: fixed;
    bottom: 10rpx;
    left: 20%;
    background: #0091FF;
    color: #fff;
    text-align: center;
}
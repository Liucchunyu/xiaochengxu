.search-filter {
    position: relative;
    z-index: 5
}

.search-filter .filter-input-wrap {
    padding: 18rpx 32rpx;
    background-color: #fff;
    position: relative
}

.search-filter .filter-input-wrap image {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 36rpx;
    left: 62rpx;
}

.search-filter .filter-input-wrap .clear-btn {
    left: auto;
    right: 62rpx;
}

.search-filter .filter-input-wrap .icon-search {
    position: absolute;
    top: 30rpx;
    font-size: 28rpx;
    left: 72rpx
}

.search-filter .filter-input-wrap input {
    background-color: #F7F7F7;
    border-radius: 32rpx;
    padding: 0 80rpx;
    height: 64rpx
}

.search-filter .filter-input-wrap .icon-shanchu {
    font-size: 28rpx;
    color: #999;
    z-index: 1
}

.search-filter .filter-input-wrap .reset-button {
    display: inline-block;
    position: absolute;
    top: 10rpx;
    right: 54rpx;
    padding: 20rpx;
    background-color: transparent;
    line-height: 1;
    border-radius: none
}

.city-location {
    width: 100vw;
    height: 100vh;
    background-color: #fff;
    top: 0;
    left: 0;
    z-index: 1000;
    overflow: auto
}

.city-location.active {
    display: block
}

.city-location.noscroll {
    overflow: hidden
}

.city-list-wrapper {
    position: relative
}

.city-list-wrapper .city-list-letter {
    margin-top: -1rpx;
    background: #f7f7f7;
    height: 64rpx;
    line-height: 64rpx;
    padding-left: 31rpx;
    font-size: 28rpx;
    color: #999;
    position: relative;
}

